{
  "name": "firestore-mobx",
  "version": "1.5.0-2",
  "description": "Observable Firestore documents and collections using MobX",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "install-peers": "install-peers -f",
    "build": "rollup -c",
    "release": "yarn build && np",
    "emulate": "firebase emulators:start --only firestore",
    "test": "jest --env node --maxWorkers 1",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,json,yml}\" \"*.{js,json,md,yml}\"",
    "preinstall": "touch ./node_modules/.metadata_never_index",
    "postinstall": "yarn install-peers"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x80/firestore-mobx.git"
  },
  "author": "Thijs Koerselman <thijs@vauxlab.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x80/firestore-mobx/issues"
  },
  "homepage": "https://github.com/0x80/firestore-mobx#readme",
  "peerDependencies": {
    "firebase": "^7.0.0",
    "mobx": "^5.0.0"
  },
  "dependencies": {
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@firebase/testing": "^0.20.9",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.158",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "firebase-tools": "^8.6.0",
    "install-peers": "^1.0.3",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.1.0",
    "lodash": "^4.17.19",
    "np": "^6.3.2",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}
