{
  "name": "firestore-mobx",
  "version": "1.0.0",
  "description": "Observable Firestore documents and collections using MobX",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "np": {
    "contents": "dist"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "release": "yarn build && np",
    "firestore-emulator": "gcloud beta emulators firestore start --host-port localhost:8080",
    "test": "FIRESTORE_EMULATOR_HOST=localhost:8080 jest",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,json,yml}\" \"*.{js,json,md,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x80/firestore-mobx.git"
  },
  "author": "Thijs Koerselman <thijs@vauxlab.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x80/firestore-mobx/issues"
  },
  "homepage": "https://github.com/0x80/firestore-mobx#readme",
  "dependencies": {
    "firebase": "^6.6.1",
    "mobx": "^5.11.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@firebase/firestore-types": "^1.5.0",
    "@types/jest": "^24.0.18",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "firebase-admin": "^8.6.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  }
}
