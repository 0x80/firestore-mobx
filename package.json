{
  "name": "firestore-mobx",
  "version": "1.3.1",
  "description": "Observable Firestore documents and collections using MobX",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "release": "yarn build && np --no-yarn",
    "emulate": "firebase emulators:start --only firestore",
    "test": "jest --env node --maxWorkers 1",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,json,yml}\" \"*.{js,json,md,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x80/firestore-mobx.git"
  },
  "author": "Thijs Koerselman <thijs@vauxlab.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x80/firestore-mobx/issues"
  },
  "homepage": "https://github.com/0x80/firestore-mobx#readme",
  "peerDependencies": {
    "firebase": "^7.6.0",
    "mobx": "^5.0.0"
  },
  "dependencies": {
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@firebase/firestore-types": "^1.10.1",
    "@firebase/testing": "^0.19.2",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-jest": "^25.4.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "firebase-tools": "^8.1.1",
    "install-peers": "^1.0.3",
    "jest": "^25.4.0",
    "lodash": "^4.17.15",
    "np": "^6.2.2",
    "prettier": "^2.0.5",
    "rollup": "^1.30.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  }
}
